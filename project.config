[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group cea-hpc
	owner = group user/Quentin Bouget (ypsah)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Quentin Bouget (ypsah)
	label-Verified = -1..+1 group user/Quentin Bouget (ypsah)
	push = group user/Quentin Bouget (ypsah)
	read = group user/Quentin Bouget (ypsah)
	rebase = group user/Quentin Bouget (ypsah)
	removeReviewer = group user/Quentin Bouget (ypsah)
	submit = group user/Quentin Bouget (ypsah)
[access "refs/heads/*"]
	create = group user/Quentin Bouget (ypsah)
	push = +force group user/Quentin Bouget (ypsah)
	pushMerge = group user/Quentin Bouget (ypsah)
	read = group user/Quentin Bouget (ypsah)
[access "refs/tags/*"]
	push = group user/Quentin Bouget (ypsah)
	push = +force group user/Quentin Bouget (ypsah)
